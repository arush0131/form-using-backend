<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Table View</title>
</head>
<body>
  <h1>Data Table</h1>
  <table border="1">
    <thead>
      <tr>
        <th>#</th>
        <th>Field</th>
        <th>Value</th>
      </tr>
    </thead>
    <tbody>
      <% data.forEach((entry, index) => { %>
        <tr>
          <td colspan="3"><strong>Submission <%= index + 1 %></strong></td>
        </tr>
        <% for (let key in entry) { %>
          <tr>
            <td></td>
            <td><%= key %></td>
            <td><%= entry[key] %></td>
          </tr>
        <% } %>
      <% }); %>
    </tbody>
  </table>
  <a href="/">Back to Form</a>
</body>
</html>
